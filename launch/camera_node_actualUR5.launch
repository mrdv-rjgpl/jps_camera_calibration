<?xml version="1.0"?>

<launch>
  <arg name="limited" default="false"/>
  <arg name="paused" default="false"/>
  <arg name="gui" default="false"/>
  
  <!-- startup simulated world -->
 <param name="/ur_driver/prevent_programming" value="true"/>
  
  <include file="$(find ur_modern_driver)/launch/ur5_bringup_compatible.launch">
    <arg name="robot_ip" value="172.22.22.2"/>
  </include>
  


   <node pkg="image_proc"
        type="image_proc"
        name="image_proc"
	output="screen"
	ns="camera">
  </node>




  <node pkg="rviz"
	type="rviz"
	name="rviz"
	args="-d $(find assignment2)/assignment2.rviz"/>

  <include file="$(find assignment2)/launch/usbcam.launch"/>



</launch>
